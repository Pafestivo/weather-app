{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base",
        "prettier"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-console": "off",
        "no-use-before-define": "off",
        "import/prefer-default-export": "off",
        "import/no-mutable-exports": "off",
        "no-unused-vars": "off",
        "consistent-return": "off",
        "no-plusplus": "off",
        "prefer-const": "off",
        "grouped-accessor-pairs": "off",
        "no-param-reassign": "off",
        "no-alert": "off"
    }
  }
  